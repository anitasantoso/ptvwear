<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="as.com.au.ptvwear" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="as.com.au.ptvwear.MainActivity_"
            android:label="PTWear" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="as.com.au.ptvwear.StopsActivity_"
            android:label="Nearby Stops" >
        </activity>
        <activity
            android:name="as.com.au.ptvwear.StopDetailsActivity_"
            android:label="Lines" >
        </activity>
    </application>
</manifest>
